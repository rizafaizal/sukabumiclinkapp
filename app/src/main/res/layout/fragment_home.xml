                                android:layout_height="wrap_content"
                                android:layout_below="@+id/ln_satgas_dash" />

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="18dp"
                        android:background="@color/semiWhiteTextColor">

                        <LinearLayout
                            android:id="@+id/ln_education_dash"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30dp"
                            android:layout_marginEnd="30dp"
                            android:orientation="horizontal">

                            <TextView
                                style="@style/TextAppearance.AppCompat.Headline"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="left"
                                android:text="@string/menu_education"
                                android:textSize="15sp"
                                android:textStyle="bold"
                                tools:ignore="RtlHardcoded" />

                            <TextView
                                android:id="@+id/tv_education_all_dash"
                                style="@style/TextAppearance.AppCompat.Body2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:text="Lihat Semua >"
                                android:textColor="@color/color4"
                                android:textStyle="bold"
                                tools:ignore="RtlHardcoded" />

                        </LinearLayout>

                        <include
                            android:id="@+id/inc_layout_education"
                            layout="@layout/layout_educationdash"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/ln_education_dash" />

                    </RelativeLayout>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/card_boulevard_dash"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="8dp"
                        android:background="@color/cardColor"
                        app:cardElevation="3dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp">

                            <LinearLayout
                                android:id="@+id/ln_boulevard_dash"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="30dp"
                                android:layout_marginEnd="30dp"
                                android:orientation="horizontal">

                                <TextView
                                    style="@style/TextAppearance.AppCompat.Headline"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="left"
                                    android:text="@string/menu_boulevard"
                                    android:textSize="15sp"
                                    android:textStyle="bold"
                                    tools:ignore="RtlHardcoded" />

                                <TextView
                                    android:id="@+id/tv_boulevard_all_dash"
                                    style="@style/TextAppearance.AppCompat.Body2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="right"
                                    android:text="Lihat Semua >"
                                    android:textColor="@color/color4"
                                    android:textStyle="bold"
                                    tools:ignore="RtlHardcoded" />

                            </LinearLayout>

                            <include
                                android:id="@+id/inc_layout_ccraft"
                                layout="@layout/layout_boulevarddash"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/ln_boulevard_dash" />

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                </LinearLayout>

            </RelativeLayout>

        </ScrollView>

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>